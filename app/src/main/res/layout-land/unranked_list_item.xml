<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/unranked_icon_imageview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingRight="12dp"/>
    <TextView
        android:id="@+id/unranked_summoner_textview"
        android:layout_width="140dp"
        android:layout_height="match_parent"
        android:textColor="@color/white"
        />
    <TextView
        android:id="@+id/unranked_wins_textview"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:textColor="@color/white"
        />
    <TextView
        android:id="@+id/unranked_kills_textview"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:textColor="@color/white"
        />
    <TextView
        android:id="@+id/unranked_assists_textview"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:textColor="@color/white"
        />
    <TextView
        android:id="@+id/unranked_minions_textview"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:textColor="@color/white"
        />
    <TextView
        android:id="@+id/unranked_neutrals_textview"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:textColor="@color/white"
        />
    <TextView
        android:id="@+id/unranked_towers_textview"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:textColor="@color/white"
        />

</LinearLayout>